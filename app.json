{
  "expo": {
    "name": "AppKu",
    "slug": "appku",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo-app.png",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to scan barcodes.",
        "NSLocationWhenInUseUsageDescription": "This app uses your location to provide better services."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo-app.png"
      },
      "package": "com.acmad01.appku",
      "permissions": [
        "ACCESS_NETWORK_STATE",
        "INTERNET",
        "CAMERA",
        "RECORD_AUDIO",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/logo-app.png"
    },
    "extra": {
      "eas": {
        "projectId": "d2b8a301-37ab-47cb-8873-9bddf8053aac"
      }
    },
    "owner": "acmad01",
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow AppKu to use your location."
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow AppKu to access your camera.",
          "microphonePermission": "Allow AppKu to access your microphone.",
          "recordAudioAndroid": true
        }
      ]
    ]
  }
}
